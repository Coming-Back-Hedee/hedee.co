        <div  id="form_versement">
                {{ form_start(form) }}
                    {{ form_label(form.proprietaire) }}
                    {{ form_widget(form.proprietaire) }} 
                    {{ form_label(form.swiftBic) }}
                    {{ form_widget(form.swiftBic) }} 
                    {{ form_label(form.iban) }}
                    {{ form_widget(form.iban) }}
                    <div class="vanish">
